{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["src/index.ts"],
  "out": "docs/api",
  "name": "Bridge API Documentation",
  "readme": "none",
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "includeVersion": true,
  "categorizeByGroup": true,
  "categoryOrder": [
    "Parsers",
    "Generators",
    "IR Types",
    "Utilities",
    "*"
  ],
  "navigationLinks": {
    "GitHub": "https://github.com/bridge-codes/bridge",
    "npm": "https://www.npmjs.com/package/@bridge/core"
  },
  "exclude": [
    "**/node_modules/**",
    "**/tests/**",
    "**/dist/**"
  ],
  "tsconfig": "./tsconfig.json",
  "plugin": [],
  "theme": "default",
  "hideGenerator": true,
  "searchInComments": true,
  "cleanOutputDir": true
}
